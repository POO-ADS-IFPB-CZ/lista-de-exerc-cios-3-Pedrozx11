import java.util.Arrays;
import java.util.Scanner;

public class MegaSenadaVirada{
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        int[] numeros = new int[6];
        int count = 0;
        while (count < 6) {
            System.out.print("Digite um número entre 1 e 60:");
            int num = scanner.nextInt();
            if (num < 1 || num > 60) {
                System.out.println("Escreva direito");
                continue;
            }
            boolean duplicado=false;
            for (int i=0;i<count; i++){
                if (numeros[i] == num){
                    duplicado = true;
                    break;
                }
            }
            if (duplicado){
                System.out.println("Repetiu o número");
            } else{
                numeros[count]=num;
                count++;
            }
        }
        Arrays.sort(numeros);
        System.out.print("Números escolhidos em ordem crescente: ");
        System.out.println(Arrays.toString(numeros));
        scanner.close();
    }
}
